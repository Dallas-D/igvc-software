<!-- rosbag.launch -->
<!--
    This file will launch the rosbag recorder and will log all topics
    excluding some of the duplicate image_transport topics.
    -->
<launch>
    <node pkg="igvc" type="system_stats" name="system_stats" output="screen">
        <param name="frequency" value="10"/>
    </node>

    <node pkg="rosbag" type="record" name="record" output="screen"
    args="/battery /encoders /fix /imu  /joint_states /lights /motors /pc2 /scan/pointcloud /robot_enabled /tf /tf_static /usb_cam_center/camera_info /usb_cam_center/image_raw /vel /waypoint /time_reference"/>
</launch>
